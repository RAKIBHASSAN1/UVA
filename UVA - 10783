#include<bits/stdc++.h>
using namespace std;
int main()
{
	  long int t,i,j,a,b,sum=0;
	cin>>t;
	for(i=1;i<=t;i++)
	{
	    cin>>a;
	    cin>>b;
	    sum=0;
	    for(j=a;j<=b;j++)
	    {
	        if(j%2!=0)
	        {
	            sum=sum+j;
	        }
	    }
	    cout<<"Case "<<i<<": "<<sum<<endl;
	}
}
